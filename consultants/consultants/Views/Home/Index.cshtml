@{
    ViewBag.Title = "CONSULTANTS ASSIGNMENTS HOME";
}

@{
    ViewBag.Title = "Play";
}

<div id="bodyContainer" ng-app="ConsApp" ng-controller="ConsCtrl">
    
            <div class="table-responsive">
                <table class="table">
                    <thead>
                        <tr>
                            <th>Consultant Name</th>
                            <th>Assignment_Name</th>
                            <th>Client</th>

                            <th>Percentage</th>
                            <th>StartDate</th>
                            <th>EndDate</th>
                            <th>Comment</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="(key,value)  in assignments track by key" ng-class="{activee:{{value.Is_Deadline}} }">
                            <td>{{value.Consultant_Name}}</td>
                            <td>{{value.Assignment_Name}}</td>
                            <td>{{value.Client_Name}}</td>
                            <td>{{value.Percentage}}</td>

                            <td>{{value.Start_Date}}</td>
                            <td>{{value.End_Date}}</td>
                            <td>{{value.Comment}}</td>

                           
                        </tr>
                    </tbody>
                </table>

               
                </div>
            </div>
@section scripts {
    @Scripts.Render("~/Scripts/angular.min.js")
@Scripts.Render("~/Scripts/app/consoltants-controller.js")
}
